algoritmo "semnome"
// Função :
// Autor :
// Data : 03/06/2022
// Seção de Declarações 
var
   cont,n:inteiro
inicio
      cont <-6
      enquanto (cont <=100)faca
         escreval (cont)
         n<- n+cont
         cont <- cont+2
      fimenquanto
      escreva ("O resultado é: ",n)
      
fimalgoritmo

algoritmo "semnome"
// Função :
// Autor :
// Data : 08/06/2022
// Seção de Declarações 
var
   c,n1,n2,n3:inteiro
inicio
      n1<-1
      n2<-1
      n3<-2
      escreva (n1)
      escreva (n2)
      para c<-2 ate 10 faca
           escreva (n3)
           n1<-n2
           n2<-n3
           n3<-n1+n2
      fimpara
      
fimalgoritmo

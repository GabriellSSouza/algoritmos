algoritmo "semnome"
// Função :
// Autor :
// Data : 16/06/2022
// Seção de Declarações 
var
   N: vetor [0..9] de inteiro
   c,i: inteiro
inicio
      para c<-0 ate 9 faca
         para i <-1 ate 9 passo 2 faca
            n[c]<-5
            n[i]<-3
         fimpara
      fimpara
      para c<-0 ate 9 faca
           escreva (n[c])
      fimpara

fimalgoritmo

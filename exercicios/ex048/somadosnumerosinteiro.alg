algoritmo "semnome"
// Função :
// Autor :
// Data : 03/06/2022
// Seção de Declarações 
var
   cont,n,soma:inteiro
inicio
      cont <- 1
      enquanto (cont <=7)faca
         escreval ("Digite um numero inteiro: ")
         leia (n)
         soma <- soma+n
         cont <- cont+1
      fimenquanto
      escreval ("A soma dos números é:",soma)
fimalgoritmo

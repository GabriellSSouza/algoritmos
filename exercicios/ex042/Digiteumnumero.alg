algoritmo "semnome"
// Função :
// Autor :
// Data : 03/06/2022
// Seção de Declarações 
var
   cont,n:inteiro
inicio
      cont <- 1
      escreva ("digite um numero inteiro e positivo: ")
      leia(n)
      enquanto (cont<= n)faca
      escreval (cont)
      cont <- cont+1
      fimenquanto
      escreva (" Acabou!")
fimalgoritmo

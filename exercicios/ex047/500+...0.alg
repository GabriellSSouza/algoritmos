algoritmo "semnome"
// Função :
// Autor :
// Data : 03/06/2022
// Seção de Declarações 
var
   cont,n:inteiro
inicio
      cont <-500
      enquanto (cont>=0)faca
         escreva (cont,"+")
         n<- n+cont
         cont<- cont-50
      fimenquanto
      escreval (" O resulta é:",n)
fimalgoritmo

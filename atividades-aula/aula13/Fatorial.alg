algoritmo "semnome"
// Função :
// Autor :
// Data : 09/06/2022
// Seção de Declarações 
var
  N,F:inteiro
funcao  fatorial (v:inteiro):inteiro
var
  c,r:inteiro
inicio
   R<-1
   para C<-1 ate V faca
      R<- R*C
   fimpara
   retorne R
fimfuncao
inicio
   escreva ("Digite um número: ")
   leia (n)
   F <- fatorial (N)
   escreval ("O valor de",N," é igual a",F)
fimalgoritmo
